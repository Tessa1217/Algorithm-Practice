      "source": [
        "# x를 기준으로 \n",
        "N = int(input())\n",
        "q = []\n",
        "for _ in range(N):\n",
        "  x, y = map(int, input().split())\n",
        "  q.append((x, y))\n",
        "q.sort()\n",
        "for i in q:\n",
        "  print(i[0], i[1])"
      ]
