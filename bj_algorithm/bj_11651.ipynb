      "source": [
        "# y를 기준으로\n",
        "N = int(input())\n",
        "q = []\n",
        "for _ in range(N):\n",
        "  x, y = map(int, input().split())\n",
        "  q.append((y, x))\n",
        "q.sort()\n",
        "for point in q:\n",
        "  print(point[1], point[0])"
      ]
